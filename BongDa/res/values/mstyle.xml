    <style name="bongda_text_item_small_bangxephang" parent="@style/bongda_text_item_small">
        <item name="android:textSize">@dimen/dimen_10dp</item>
        <item name="android:padding">@dimen/dimen_0dp</item>
        <item name="android:gravity">center</item>
    </style>

</resources>